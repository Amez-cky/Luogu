#include <stdio.h>
#include <math.h>

int prime(int n);

int main(void)
{
    int L;
    scanf("%d", &L);
    int i = 1, sum = 0, count = 0;
    while (1)
    {
        if (sum < L)
        {
            int j = prime(i);
            if (j == 1)
            {
                sum += i;
                count++;
                if (sum > L)
                {
                    sum -= i;
                    count--;
                    break;
                }
                printf("%d\n", i);
            }
            i++;
        }
        else
        {
            break;
        }
    }
    printf("%d", count);

    return 0;
}

int prime(int n)
{
    if (n <= 1)
    {
        return 0;
    }
    else if (n == 2)
    {
        return 1;
    }
    else if (n % 2 == 0)
    {
        return 0;
    }
    for (int i = 3; i <= sqrt(n); i += 2)
    {
        if (n % i == 0)
        {
            return 0;
        }
    }
    return 1;
}
