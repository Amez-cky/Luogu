#include <stdio.h>

int main() {
    // 定义三个整数变量
    int a, b, c;
    // 定义用于交换的临时变量
    int temp;

    // 输入三个整数
    scanf("%d %d %d", &a, &b, &c);

    // 排序（冒泡排序思想，进行两两比较交换）
    if (a > b) {
        temp = a;
        a = b;
        b = temp;
    }
    if (a > c) {
        temp = a;
        a = c;
        c = temp;
    }
    if (b > c) {
        temp = b;
        b = c;
        c = temp;
    }

    // 输出排序后的结果
    printf("%d %d %d\n", a, b, c);

    return 0;
}
